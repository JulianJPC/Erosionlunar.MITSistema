@model List<Erosionlunar.MITSistema.Abstract.ABSProcesador>

@{
    ViewData["Title"] = "Procesar Carpeta 3";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Lista Archivos Procesados</h1>

    <table class="table table-striped table-hover">
        <thead>
            <tr>
                <th>
                    Nombre en Disco
                </th>
                <th>
                    folio I
                </th>
                <th>
                    folio F
                </th>
                <th>
                    asiento I
                </th>
                <th>
                    asiento F
                </th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>
                        <p> @Model[i].getNombreEnDisco() </p>
                    </td>
                    <td>
                        <p> @Model[i].getFolioI() </p>
                    </td>
                    <td>
                        <p> @Model[i].getFolioF() </p>
                    </td>
                <td>
                    @if(@Model[i].getAsientoF() > 20){
                    <p> @Model[i].getAsientoI() </p>
                    }
                    else{
                        <p>-</p>
                    }
                    
                </td>
                <td>
                    @if(@Model[i].getAsientoF() > 20){
                    <p> @Model[i].getAsientoF() </p>
                    }
                    else{
                    <p>-</p>
                    }
                </td>
                </tr>
            }
        </tbody>
    </table>